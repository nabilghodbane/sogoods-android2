package com.hitechno.sogoods.models;

import java.util.ArrayList;

public class SearchMenuLeft {
	public ArrayList<Brand> lstBrands;
	public ArrayList<Profile> lstProfiles;
	public ArrayList<Product> lstProducts;
	public ArrayList<Store> lstStores;

	public SearchMenuLeft() {
		lstBrands = new ArrayList<Brand>();
		lstProfiles = new ArrayList<Profile>();
		lstProducts = new ArrayList<Product>();
		lstStores = new ArrayList<Store>();
	}

	public SearchMenuLeft(ArrayList<Brand> lstbrand,
			ArrayList<Profile> lstprofile, ArrayList<Product> lstProduct,
			ArrayList<Store> lstStore) {
		this.lstBrands = lstbrand;
		this.lstProfiles = lstprofile;
		this.lstProducts = lstProduct;
		this.lstStores = lstStore;

	}
}
