<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:sogoods="http://schemas.android.com/apk/res/com.hitechno.sogoods"
	android:layout_width="match_parent" android:layout_height="match_parent"
	android:orientation="vertical" >

	<ListView android:id="@+id/profile_quiz_list"
		android:layout_width="match_parent" android:layout_height="0dp"
		android:layout_weight="9" android:listSelector="@android:color/transparent" />

	<com.hitechno.sogoods.views.FuturaButton
		android:id="@+id/submit_profile_button" android:layout_width="match_parent"
		android:layout_height="0dp" android:layout_weight="1"
		android:layout_margin="10dp" sogoods:typeface="futura_std_medium"
		android:text="@string/submit_profile" />
</LinearLayout>